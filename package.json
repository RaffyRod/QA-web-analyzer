{
  "name": "qa-web-analyzer",
  "version": "1.0.0",
  "description": "Web accessibility analyzer for images, links, buttons, inputs, and ARIA elements",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsc --watch & node --watch dist/server.js",
    "type-check": "tsc --noEmit",
    "run": "node start.js"
  },
  "keywords": [
    "accessibility",
    "qa",
    "analyzer",
    "aria",
    "alt",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "playwright": "^1.40.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3"
  }
}
